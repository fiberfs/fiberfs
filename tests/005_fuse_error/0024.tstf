fiber_test "Error during concurrency"

# Init

sys_mkdir_tmp
fs_test_fuse_mount $sys_tmpdir
fs_test_dentry_ttl_ms 0
fs_test_allow_crash
fs_test_fuse_init_root

print "### Test 1"

set_var1 "cd " $sys_tmpdir "; sleep 0.01; cat * >/dev/null 2>&1 || true"
set_var2 "cd " $sys_tmpdir "; sleep 0.01; cat */* >/dev/null 2>&1 || true"
set_var3 "cd " $sys_tmpdir "; sleep 0.011; stat __CRASH >/dev/null 2>&1 || true"

shell_bg $var1
shell_bg $var2
shell_bg $var3

shell_waitall

sleep_ms 100

fuse_test_unmount
